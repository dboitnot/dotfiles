##
## Office 365 via davmail
##

account          sig
host             smtp.office365.com
port             587
tls              on
tls_starttls     on
tls_certcheck    on
auth             xoauth2
user             boitnott@sigcorp.com
passwordeval     "python ~/.config/mu4e/mutt_oauth2.py ~/.config/mu4e/sig-token.gpg"
from             boitnott@sigcorp.com

##
## Gmail
##

account          gmail
tls              on
auth             xoauth2
host             smtp.gmail.com
port             587
user             dboitnot@gmail.com
from             dboitnot@gmail.com
passwordeval     "python ~/.config/mu4e/mutt_oauth2.py ~/.config/mu4e/gmail-token.gpg"
